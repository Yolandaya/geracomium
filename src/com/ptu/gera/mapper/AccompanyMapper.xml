<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ptu.gera.mapper.AccompanyMapper">  <!-- 要映射的mapper的完整包+mapper名称-->
    <select id="queryAllAccompany" resultType="Accompany">
        select * from accompany
    </select>
    <insert id="insertAccompany" >
        insert into reserveaccompany (auser_id,aaccompany_id)
        values(#{0},#{1})
    </insert>
</mapper>